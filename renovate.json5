{
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "app*.yaml",
      ],
      "matchStrings": [
        "repoURL: europe-docker.pkg.dev/test/vendor-helm-charts/.+\\s+chart:\\s*(?<packageName>\\S+)\\s+targetRevision:\\s*(?<currentValue>\\S+)",
        "!repoURL: europe-docker.pkg.dev/test/vendor-helm-charts/.+\\s+chart:\\s*thanos\\s+targetRevision:\\s*(?<currentValue>\\S+)",
      ],
      "datasourceTemplate": "helm",
      "depNameTemplate": "helm-oci/{{{packageName}}}",
    },

    {
      "customType": "regex",
      "managerFilePatterns": [
        "app-thanos.yaml",
      ],
      "matchStrings": [
        "repoURL: europe-docker.pkg.dev/test/vendor-helm-charts/.+\\s+chart:\\s*(?<packageName>\\S+)\\s+targetRevision:\\s*(?<currentValue>\\S+)",
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "helm-docker/{{{packageName}}}",
    },
  ],

  "packageRules": [
    {
      "matchDepNames": ["helm-oci/cert-manager"],
      "changelogUrl": "https://github.com/cert-manager/cert-manager",
      "registryUrls": ["https://charts.jetstack.io"],
    },
    {
      "matchDepNames": ["helm-docker/thanos"],
      "changelogUrl": "https://github.com/bitnami/charts",
      "registryUrls": ["oci://registry-1.docker.io/bitnamicharts/thanos"],
    },
  ],
}
